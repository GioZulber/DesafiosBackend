<h1>CoderChat</h1>
<div class='container-form' id="container-form">
  <h3>Debe registrarse para continuar</h3>
   <form>
      <div class="form" id="form">
        <label for="name">Nombre</label>
        <input type="text" id="name" name="name" placeholder="Nombre" required>
        <label for="lastname">Apellido</label>
        <input type="text" id="lastName" name="lastname" placeholder="Apellido" required>
        <label for="email">Email</label>
        <input type="email" id="email" name="email" placeholder="Email" required>
        <label for="age">Edad</label>
        <input type="number" id="age" name="age" placeholder="Edad" required>
        <label for="nickname">Nickname</label>
        <input type="text" id="nickname" name="nickname" placeholder="Nickname" required>
        <label for="avatar">avatar</label>
        <input type="text" id="avatar" name="avatar" placeholder="avatar" required>
        <button type="submit" id="register" class="button-back">Registrar</button>
    </div>
   </form>
 </div> 
</div>
<div class="container-chat" id="container-chat">
  <h3 id="percentTitle"></h3>
  <div id="chatAndInput" class="chatAndInput">
        <div id="chat" class="chat"></div>
        <div id="input" class="input">
          <input
            id="chatBox"
            type="text"
            class="inputText"
            laceholder="Escribe tu mensaje aqui"
          />
          <button id="send" class="buttonEnviar">Enviar</button>
        </div>
      </div>
</div>
<script
  src='https://cdn.jsdelivr.net/npm/normalizr@3.6.1/dist/normalizr.browser.min.js'
></script>
<script src='//cdn.jsdelivr.net/npm/sweetalert2@11'></script>
<script src='/socket.io/socket.io.js'></script>
<script src='./index.js'></script>
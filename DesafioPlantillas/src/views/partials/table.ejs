<div>
  <h1 class="text-danger">Historial</h1>
  <% if(products.length === 0){ %>
  <h2 class="bg-danger rounded text-center m-4">No se encontraron datos</h2>
  <% } else { %>
  <table class="table table-dark rounded-8">
    <tr class="text-warning">
      <th scope="col">Nombre</th>
      <th scope="col">Apellido</th>
      <th scope="col">edad</th>
    </tr>

    <% products.forEach(function(){ %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.surname %></td>
      <td><%= user.age %></td>
    </tr>
    <%});%>
  </table>
  <% } %>
</div>
